{
  "manifest_version": 2,

  "name": "Waterloo/Laurier Start Page",
  "description": "This extension is a new tab home page for double degree students at Waterloo/Laurier",
  "version": "2.0",

  "browser_action": {
    "default_icon": "waterloo.png"
  },
  "chrome_url_overrides" : {
   "newtab": "index.html"
 },
 "content_scripts": [
       {
           "matches": ["<all_urls>"],
           "js": [
             "./js/script.js",
             "./js/jquery-3.1.0.min.js",
             "./js/bootstrap.min.js"]
       }
   ],
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
